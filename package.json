{
  "name": "epochalypsenow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run web-dev",
    "lint": "eslint -f stylish src/  *.js",
    "lint:css": "stylelint './src/**/*.js'",
    "web-dev": "webpack-dev-server --config webpack.config.js",
    "webpack:dev": "webpack --config webpack.config.js",
    "webpack:prod": "NODE_ENV=production webpack --config webpack.config.js",
    "postmerge": "npm install",
    "analyzeBundleSize": "NODE_ENV=production webpack --config ./webpack.config.js --json | webpack-bundle-size-analyzer",
    "build": "npm run webpack:prod",
    "deploy": "npm run build && aws s3 sync ./dist s3://epochalypsenow --delete",
    "test": "BABEL_ENV=test NODE_ENV=test mocha -r babel-register -r babel-polyfill -r test/Setup.js --recursive test/unit/*",
    "test:coverage": "BABEL_ENV=test-coverage NODE_ENV=test nyc mocha -r babel-register -r babel-polyfill -r test/Setup.js --recursive test/unit/*",
    "test:watch": "npm run test -- -watch"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-plugin-react": "^7.10.0",
    "favicons-webpack-plugin": "0.0.9",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "imagemin-webpack-plugin": "^2.1.5",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "mock-local-storage": "^1.0.5",
    "nyc": "^12.0.2",
    "react-addons-test-utils": "^15.6.2",
    "robotstxt-webpack-plugin": "^4.0.1",
    "sinon": "^6.1.4",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "webpack": "^4.14.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "date-fns": "^1.29.0",
    "es6-promise": "^4.2.2",
    "i18next": "^11.4.0",
    "i18next-localstorage-cache": "^1.1.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "polished": "^1.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.3",
    "react-i18next": "^7.9.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-toggle": "^4.0.2",
    "styled-components": "^3.3.3"
  }
}
