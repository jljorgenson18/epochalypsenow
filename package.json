{
  "name": "epochalypsenow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run web-dev",
    "lint": "eslint -f stylish src/  *.js",
    "lint:css": "stylelint './src/**/*.js'",
    "web-dev": "webpack-dev-server",
    "webpack:dev": "webpack",
    "webpack:prod": "NODE_ENV=production webpack",
    "webpack:analyzer": "NODE_ENV=analysis webpack",
    "precommit": "npm test",
    "postmerge": "npm install",
    "build": "npm run webpack:prod",
    "deploy": "npm run build && aws s3 sync ./dist s3://epochalypsenow --delete",
    "test": "BABEL_ENV=test NODE_ENV=test mocha -r babel-register -r babel-polyfill -r test/Setup.js --recursive test/unit/*",
    "test:coverage": "BABEL_ENV=test-coverage NODE_ENV=test nyc mocha -r babel-register -r babel-polyfill -r test/Setup.js --recursive test/unit/*",
    "test:watch": "npm run test -- -watch --reporter='landing'",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/builds/index.bundle.js",
      "webpack": false,
      "limit": "250 KB"
    }
  ],
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "favicons-webpack-plugin": "0.0.9",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "imagemin-webpack-plugin": "^2.2.0",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "react-addons-test-utils": "^15.6.2",
    "robotstxt-webpack-plugin": "^4.0.1",
    "sinon": "^6.1.5",
    "size-limit": "^0.19.1",
    "style-loader": "^0.22.1",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "accounting": "^0.4.1",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "date-fns": "^1.29.0",
    "es6-promise": "^4.2.2",
    "i18next": "^11.6.0",
    "i18next-localstorage-cache": "^1.1.1",
    "lodash": "^4.17.10",
    "material-ui-pickers": "^1.0.0-rc.12",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "polished": "^1.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.4.2",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.4",
    "react-i18next": "^7.10.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.28.2",
    "styled-components": "^3.4.2"
  }
}
