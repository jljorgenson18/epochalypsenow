{
  "name": "epochalypsenow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run web-dev\" \"npm run lint:watch\"",
    "lint": "eslint -f stylish src/  *.js",
    "lint:watch": "esw -f simple-detail -w --changed src/ *.js",
    "lint:css": "stylelint './src/**/*.js'",
    "web-dev": "webpack-dev-server --config webpack.config.js",
    "webpack:dev": "webpack --config webpack.config.js",
    "webpack:prod": "NODE_ENV=production webpack --config webpack.config.js",
    "precommit": "npm test",
    "postmerge": "npm install",
    "analyzeBundleSize": "NODE_ENV=production webpack --config ./webpack.config.js --json | webpack-bundle-size-analyzer"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-styled-components": "^1.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.3.1",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "imagemin-webpack-plugin": "^1.6.1",
    "json-loader": "^0.5.7",
    "robotstxt-webpack-plugin": "^4.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.4.0",
    "stylelint-processor-styled-components": "^1.2.2",
    "webpack": "^3.10.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.11.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "date-fns": "^1.29.0",
    "es6-promise": "^4.2.2",
    "lodash": "^4.17.4",
    "polished": "^1.9.0",
    "promise.prototype.finally": "^3.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0-beta.13",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "styled-components": "^2.4.0"
  }
}
